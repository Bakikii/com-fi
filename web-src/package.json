{
    "name": "@void-tech/comfy-ts",
    "version": "0.0.4",
    "main": "./dist/app.js",
    "type": "module",
    "scripts": {
        "vite:dev": "vite",
        "vite:build": "tsc && vite build",
        "vite:preview": "vite preview",
        "dependency-test": "madge --circular ./src/scripts/app.ts",
        "build": "yarn clean && tsc && yarn copy-js && yarn copy-public",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "copy-js": "cpx \"web-src/src/**/*.{js,example,css}\" web/ --preserve",
        "copy-public": "cpx \"web-src/public/**/*.*\" web/",
        "clean": "rimraf ./web/",
        "dev": "http-server ./web-src -c-1 -o",
        "start": "http-server ./web -c-1 -o"
    },
    "devDependencies": {
        "@types/node": "^16.11.16",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "cpx": "^1.5.0",
        "eslint": "^8.5.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "http-server": "^13.0.1",
        "madge": "^4.0.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.4",
        "vite": "^2.6.4"
    },
    "dependencies": {
        "@vitejs/plugin-react": "^4.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "license": "MIT"
}
